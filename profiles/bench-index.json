{
  "indexing": {
    "gitBlame": false,
    "lint": false,
    "complexity": false,
    "riskAnalysis": false,
    "riskAnalysisCrossFile": false,
    "typeInference": false,
    "typeInferenceCrossFile": false,
    "typescriptParser": "babel",
    "typescript": {
      "importsOnly": true,
      "embeddingBatchMultiplier": 4
    },
    "postings": {
      "enableChargrams": false
    },
    "shards": {
      "enabled": true,
      "minFiles": 1,
      "dirDepth": 0
    },
    "twoStage": {
      "enabled": true,
      "background": true,
      "queue": true
    },
    "embeddings": {
      "enabled": true,
      "mode": "service"
    }
  }
}
