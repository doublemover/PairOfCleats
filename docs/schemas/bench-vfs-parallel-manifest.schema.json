{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "pairofcleats://schemas/bench-vfs-parallel-manifest.schema.json",
  "title": "Parallel VFS Manifest Bench Output",
  "type": "object",
  "required": ["generatedAt", "segments", "segmentBytes", "samples", "scenarios"],
  "properties": {
    "generatedAt": { "type": "string" },
    "segments": { "type": "integer", "minimum": 1 },
    "segmentBytes": { "type": "integer", "minimum": 1 },
    "samples": { "type": "integer", "minimum": 1 },
    "scenarios": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "required": [
          "concurrency",
          "segments",
          "segmentBytes",
          "rows",
          "stats",
          "totalMs",
          "rowsPerSec"
        ],
        "properties": {
          "concurrency": { "type": "integer", "minimum": 1 },
          "segments": { "type": "integer", "minimum": 1 },
          "segmentBytes": { "type": "integer", "minimum": 1 },
          "rows": { "type": "integer", "minimum": 0 },
          "stats": {
            "type": "object",
            "required": ["count", "mean", "min", "max", "p50", "p95", "p99"],
            "properties": {
              "count": { "type": "number", "minimum": 1 },
              "mean": { "type": "number", "minimum": 0 },
              "min": { "type": "number", "minimum": 0 },
              "max": { "type": "number", "minimum": 0 },
              "p50": { "type": "number", "minimum": 0 },
              "p95": { "type": "number", "minimum": 0 },
              "p99": { "type": "number", "minimum": 0 }
            },
            "additionalProperties": false
          },
          "totalMs": { "type": "number", "minimum": 0 },
          "rowsPerSec": { "type": "number", "minimum": 0 }
        },
        "additionalProperties": true
      }
    }
  },
  "additionalProperties": true
}
