{
  "excludedDirs": [
    "fixtures",
    "helpers",
    ".testLogs",
    ".testCache",
    ".logs",
    ".cache",
    ".worktrees",
    "worktree",
    "worktrees"
  ],
  "excludedFiles": ["run.js"],
  "knownLanes": ["smoke", "unit", "ci-lite", "ci-long", "integration", "services", "api", "storage", "perf", "ci"],
  "laneRules": [
    { "lane": "perf", "match": ["^perf/"] },
    { "lane": "ci-lite", "match": ["^ci-lite/"] },
    { "lane": "smoke", "match": ["^smoke/"] },
    { "lane": "api", "match": ["^services/api/"] },
    { "lane": "services", "match": ["^services/"] },
    { "lane": "storage", "match": ["^storage/"] },
    {
      "lane": "unit",
      "match": [
        "^shared/",
        "^tooling/",
        "^lang/",
        "^cli/",
        "^runner/",
        "^indexer/"
      ]
    }
  ],
  "tagRules": [
    { "tag": "perf", "match": ["^perf/"] },
    { "tag": "services", "match": ["^services/"] },
    { "tag": "api", "match": ["^services/api/"] },
    { "tag": "storage", "match": ["^storage/"] },
    { "tag": "indexing", "match": ["^indexing/"] },
    { "tag": "retrieval", "match": ["^retrieval/", "^retrieval-"] },
    { "tag": "lang", "match": ["^lang/"] },
    { "tag": "tooling", "match": ["^tooling/"] },
    { "tag": "harness", "match": ["^runner/harness/"] },
    { "tag": "bench", "match": ["^perf/bench/"] },
    { "tag": "smoke", "match": ["^smoke/"] },
    { "tag": "sqlite", "match": ["sqlite"] },
    { "tag": "lmdb", "match": ["lmdb"] },
    { "tag": "mcp", "match": ["mcp"] },
    { "tag": "api", "match": ["^api-server"] },
    { "tag": "watch", "match": ["^indexing/watch/"] },
    { "tag": "embeddings", "match": ["embeddings"] },
    { "tag": "tooling", "match": ["tooling|lsp|type-inference"] },
    {
      "tag": "long",
      "match": [
        "^lang/fixtures-sample/python-metadata$",
        "^lang/fixtures-sample/rust-metadata$",
        "^lang/fixtures-sample/swift-metadata$",
        "^retrieval/contracts/compact-json$",
        "^retrieval/contracts/result-shape$",
        "^retrieval/filters/ext-path$",
        "^retrieval/filters/type-signature-decorator$",
        "^lang/contracts/python$",
        "(^|/)parity$",
        "(^|/)fixture-parity$",
        "(^|/)fixture-eval$",
        "(^|/)incremental-tokenization-cache$",
        "(^|/)index-config-dump$",
        "(^|/)summary-report-parity-sqlite$",
        "(^|/)config-dump$",
        "(^|/)summary-report-parity-sqlite-fts$",
        "(^|/)code-map-performance$",
        "(^|/)summary-report-compare-memory$",
        "(^|/)summary-report-compare-sqlite$",
        "(^|/)search-help$",
        "(^|/)ann-parity$",
        "(^|/)incremental-cache-signature$",
        "^retrieval/filters/behavioral$",
        "^retrieval/filters/risk$",
        "^retrieval/filters/types$",
        "(^|/)retrieval-strict-manifest-embeddings$",
        "^lang/contracts/misc-buildfiles$",
        "^retrieval/filters/control-flow$",
        "^retrieval/filters/file-selector$",
        "^lang/contracts/go$",
        "^lang/contracts/sql$",
        "^lang/contracts/javascript$",
        "^lang/contracts/typescript$",
        "(^|/)summary-report$",
        "(^|/)hnsw-ann$",
        "^indexing/language-fixture/postings-integrity$",
        "^indexing/language-fixture/chunk-meta-exists$",
        "(^|/)code-map-guardrails$",
        "(^|/)comment-join$",
        "(^|/)chunkuid-determinism$",
        "(^|/)eval-quality$",
        "(^|/)fielded-bm25$",
        "(^|/)artifact-size-guardrails$",
        "(^|/)churn-filter$",
        "(^|/)manifest-embeddings-pieces$",
        "(^|/)hnsw-atomic$",
        "(^|/)embeddings-cache-identity$",
        "^tooling/triage/records-index-and-search$",
        "(^|/)embeddings-dims-mismatch$",
        "(^|/)embeddings-validate$",
        "(^|/)repo-root$",
        "^retrieval/filters/file-and-token/file-selector-case$",
        "(^|/)symbol-artifact-determinism$",
        "(^|/)extracted-prose$",
        "(^|/)search-tie-order$",
        "(^|/)prose-skip-imports$",
        "(^|/)subprocess-quoting$",
        "(^|/)lancedb-ann$",
        "(^|/)piece-assembly$",
        "(^|/)query-cache$",
        "(^|/)search-rrf$",
        "(^|/)shard-merge$",
        "(^|/)script-coverage$"
      ]
    },
    {
      "tag": "long",
      "match": [
        "^tooling/triage/context-pack$",
        "^services/mcp/tool-search-defaults-and-filters$"
      ]
    },
    {
      "tag": "DelayedUntilPhase7_8",
      "match": ["^storage/lmdb/"]
    },
    { "tag": "destructive", "match": ["(^|/)uninstall$"] }
  ],
  "outputIgnorePatterns": ["^\\[ignore\\]\\s+read-failed\\b"]
}
