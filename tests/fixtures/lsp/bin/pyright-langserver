#!/usr/bin/env node
import { launchStubServer } from './proxy-runner.js';

const args = process.argv.slice(2);
if (args.includes('--version') || args.includes('--help')) {
  process.stdout.write('pyright-langserver stub\n');
  process.exit(0);
}

launchStubServer({ metaUrl: import.meta.url, mode: 'pyright' });
